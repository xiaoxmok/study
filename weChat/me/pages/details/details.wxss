page{
  background:#f6f6f6;
  padding-top:85rpx;
}
.page_tab{
  position:fixed;
  z-index: 99;
  top:0;
  left:0;
  width:100%;
}
.page_tab{ line-height: 85rpx; background: #fff; font-size: 0; white-space: nowrap; }
.page_tab .tab_item{ position: relative; height: 100%; display: inline-block; vertical-align: middle; padding: 0 20rpx; font-size: 28rpx; color: #666666; border-bottom: 3rpx solid #fff; }
.page_tab .tab_item.cur{ color: #f95e63; padding: 0 22rpx; border-color: #f95e63; }
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.kue_list{  margin: 15rpx 0 0 0; }
.kue_list .list_item{ position: relative; display: block; border-bottom: 1px solid #eeeeee; padding: 15rpx 50rpx 20rpx 40rpx; margin-bottom:15rpx;background: #fff;}
.kue_list .list_item .item_triangle{ position: absolute; left: -45rpx; top: -45rpx; text-align: center; border: 45rpx solid transparent; border-bottom-color: #fc4332; transform: rotate(-45deg); }
.kue_list .list_item .item_triangle.yellow{ border-bottom-color: #fd9137;}
.kue_list .list_item .item_triangle .txt{ position: absolute; left: -14rpx; top: 10rpx; color: #fff; font-size: 24rpx; font-family: simsun; transform: rotate(45deg); }
.kue_list .list_item .css_arr1{ position: absolute; top: 50%; right: 30rpx; margin-top: -10rpx; }
.kue_list .list_item .item_name{ font-size: 24rpx; line-height: 38rpx; color: #333333; }
.kue_list .list_item .item_data{ font-size: 0; padding: 20rpx 0 0 0; }
.kue_list .list_item .item_data .item_data_col{ display: inline-block; vertical-align: top; color: #666666; font-size: 24rpx; line-height: 40rpx; padding-right: 20rpx; }
.kue_list .list_item .item_data .item_data_col:nth-child(1){ width: 230rpx; }
.kue_list .list_item .item_data .item_data_col:nth-child(2){ width: 210rpx; }
.kue_list .list_item .item_data .item_data_col_num .s1{ font-size: 30rpx; color: #f95e63; }
.kue_list .list_item .item_data .item_data_col_num .s2{ font-size: 28rpx; color: #333333; }
.kue_list .list_item .item_data .item_data_col_num .del{ font-size: 24rpx; color: #999999; margin-right: 10rpx; }
.kue_list .list_item .item_data .item_data_col_txt{ font-size: 22rpx; color: #999999; }

.kue_list .usedHero{
  color:#f0aa46;
  border-bottom:1px solid #e0dfdf;height:7px;margin-bottom:30rpx;
}
.kue_list .Hero{background:#f6f6f6;width:100rpx;font-size:24rpx;padding:0 10rpx 0 10rpx;font-weight: bold;}
.show{ display: block; }
.hidden{ display: none; }

